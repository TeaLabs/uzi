{
    "name": "tea/uzi",
    "description": "A string manipulation library.",
    "keywords": [
        "multibyte", "string", "manipulation", "utility", "methods", "utf-8",
        "helpers", "utils", "utf"
    ],
    "license": "MIT",
    "homepage": "https://github.com/TeaLabs/uzi",
    "support": {
        "issues": "https://github.com/TeaLabs/uzi/issues",
        "source": "https://github.com/TeaLabs/uzi"
    },
    "authors": [
        {
            "name": "David Kyalo",
            "email": "davidmkyalo@gmail.com",
            "homepage": "http://www.davidkyalo.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-mbstring": "*",
        "tea/regex": "~0.1.0",
        "tea/future": "~0.1.0",
        "tea/contracts": "~0.1.0",
        "doctrine/inflector": "~1.0",
        "danielstjules/stringy": "~2.3",
        "symfony/polyfill-mbstring" : "~1.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Tea\\Uzi\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tea\\Tests\\Uzi\\": "tests/"
        },
        "files": [
            "tests/functions.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.0-dev"
        }
    },
    "minimum-stability": "dev"
}
