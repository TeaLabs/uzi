{
    "name": "tea/uzi",
    "description": "A string manipulation library.",
    "keywords": [
        "multibyte", "string", "manipulation", "utility", "methods", "utf-8",
        "helpers", "utils", "utf"
    ],
    "homepage": "https://github.com/davidkyalo/uzi",
    "license": "MIT",
    "authors": [
        {
            "name": "David Kyalo",
            "email": "davidmkyalo@gmail.com",
            "homepage": "http://www.davidkyalo.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "ext-mbstring": "*",
        "danielstjules/stringy": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "support": {
        "issues": "https://github.com/davidkyalo/uzi/issues",
        "source": "https://github.com/davidkyalo/uzi"
    },
    "autoload": {
        "psr-4": { "Tea\\Uzi\\": "src/" },
        "files": ["src/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tea\\Uzi\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.0-dev"
        }
    },
    "minimum-stability": "dev"
}
